Title = {The Effect of Extrusion Temperature and Cycles on Electrical Resistivity
   in Carbon Nanofiber-Modified PLA Filament for Multi-Functional Additive
   Manufacturing},
Booktitle = {SENSORS AND SMART STRUCTURES TECHNOLOGIES FOR CIVIL, MECHANICAL, AND
   AEROSPACE SYSTEMS 2020},
