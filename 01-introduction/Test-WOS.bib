@article{ WOS:000398906000008,
Author = {Dimitriadis, Alexandros I. and Debogovic, Tomislav and Favre, Mirko and
   Billod, Mathieu and Barloggio, Luca and Ansermet, Jean-Philippe and de
   Rijk, Emile},
Title = {Polymer-Based Additive Manufacturing of High-Performance Waveguide and
   Antenna Components},
Journal = {PROCEEDINGS OF THE IEEE},
Year = {2017},
Volume = {105},
Number = {4},
Pages = {668-676},
Month = {APR},
Abstract = {In this paper, a lightweight horn antenna array demonstrator is designed
   and fabricated by means of an additive manufacturing (AM) process that
   has been optimized for complicated passive radio-frequency (RF)
   components. The presented design comprises a complex waveguide feeding
   network (WFN) which aims to exploit the increased flexibility offered by
   this novel fabrication technique. This WFN concept could be easily
   extended to larger antenna arrays with higher gain for full-size
   telecommunication applications. The electromagnetic performance of the
   featured structure is validated by means of full-wave numerical
   simulations. Fabricated prototypes of the WFN and of the complete
   antenna array are tested and are found to exhibit a promising agreement
   with the simulation results.},
