@inproceedings{ WOS:000366982900028,
Author = {Tuerk, Daniel-Alexander and Zueger, Andreas and Klahn, Christoph and
   Meboldt, Mirko},
Editor = {Weber, C and Husung, S and Cantamessa, M and Cascini, G and Marjanovic, D and Graziosi, S},
Title = {COMBINING ADDITIVE MANUFACTURING WITH CFRP COMPOSITES: DESIGN POTENTIALS},
