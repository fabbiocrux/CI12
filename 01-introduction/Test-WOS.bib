@article{ WOS:000456378800026,
Author = {Roschli, Alex and Gaul, Katherine T. and Boulger, Alex M. and Post,
   Brian K. and Chesser, Phillip C. and Love, Lonnie J. and Blue, Fletcher
   and Borish, Michael},
Title = {Designing for Big Area Additive Manufacturing},
