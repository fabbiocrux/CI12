@article{ WOS:000960867600005,
Author = {Ponsuriyaprakash, S. and Udhayakumar, P. and Hemalatha, A. and Sabarish,
   S.},
Title = {Additive manufacturing of customized automotive components using novel
   cellulose fiber reinforced abs polymer filament},
Journal = {INTERNATIONAL JOURNAL OF INTERACTIVE DESIGN AND MANUFACTURING - IJIDEM},
Year = {2023},
Volume = {17},
Number = {4},
Pages = {1869-1880},
Month = {AUG},
Abstract = {This study examines the advantages of additive manufacturing over
   traditional production in terms of cost, strength, and size. The
   previous work's filament was 1.75 mm in diameter and made up of 80:20
   wt\% ABS and cellulose fiber composite. The filament was utilized in a
   3D printer to create customizable automotive parts that were developed
   with CAD software and produced as scaled models. To precisely compare
   the models, fabrication factors such as layer thickness, printing time
   and speed, and fill density were evaluated, and the components were
   printed at 100\% fill density. Tensile, flexural, and impact strengths
   of the filament material were found to be 46\%, 25\%, and 17\% greater
   than pure ABS composite, respectively. The study also demonstrated that
   additive manufacturing of customized car parts provided cost and
   strength benefits compared to conventionally manufactured components.},
