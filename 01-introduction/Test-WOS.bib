@inproceedings{ WOS:000340850500018,
Author = {Choong, Y. Y. C. and Maleksaeedi, S. and Wiria, F. E. and Su, P-C},
Editor = {Kai, CC and Yee, YW and Jen, TM and Erjia, L},
Title = {AN OVERVIEW OF MANUFACTURING POLYMER-BASED FUNCTIONALLY GRADED MATERIALS
   USING 3D STEREOLITHOGRAPHY PROCESS},
Booktitle = {PROCEEDINGS OF THE 1ST INTERNATIONAL CONFERENCE ON PROGRESS IN ADDITIVE
   MANUFACTURING},
Year = {2014},
Pages = {103+},
Note = {1st International Conference on Progress in Additive Manufacturing,
   Nanyang Execut Ctr, Singapore, SINGAPORE, MAY 26-28, 2014},
Organization = {Nanyang Technol Univ},
Abstract = {Functionally graded materials (FGM) exhibit changes in properties due to
   gradual variation in composition and structures. An overview of current
   polymer-based FGM fabricated by conventional manufacturing techniques
   and its applications are presented. However, such techniques are
   laborious and less productive in fabricating FGM. Additive manufacturing
   (AM) is introduced to produce FGM by adopting its layer by layer
   technique to control the composition and geometry of each layer and
   build complex parts directly from computer data. Existing developments
   in the field of FGM processing via different AM techniques are
   highlighted and an analysis of using 3D stereolithography (SLA) process
