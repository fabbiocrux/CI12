@article{ WOS:000415357400002,
Author = {Puppi, Dario and Chiellini, Federica},
Title = {Wet-spinning of biomedical polymers: from single-fibre production to
   additive manufacturing of three-dimensional scaffolds},
Journal = {POLYMER INTERNATIONAL},
Year = {2017},
Volume = {66},
Number = {12},
Pages = {1690-1696},
