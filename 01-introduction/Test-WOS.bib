@article{ WOS:000525355800003,
Author = {Chueh, Yuan-Hui and Zhang, Xiaoji and Wei, Chao and Sun, Zhe and Li, Lin},
Title = {Additive Manufacturing of Polymer-Metal/Ceramic Functionally Graded
   Composite Components via Multiple Material Laser Powder Bed Fusion},
