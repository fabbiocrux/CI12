@article{ WOS:000633750600001,
Author = {Rasaki, Sefiu Abolaji and Xiong, Dingyu and Xiong, Shufeng and Su, Fang
   and Idrees, Muhammad and Chen, Zhangwei},
Title = {Photopolymerization-based additive manufacturing of ceramics: A
   systematic review},
Journal = {JOURNAL OF ADVANCED CERAMICS},
Year = {2021},
Volume = {10},
