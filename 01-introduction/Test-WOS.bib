Author = {Yan, Yi and Ngo, Khai D. T. and Mei, Yuhui and Lu, Guo-Quan},
Book-Group-Author = {IEEE},
Title = {Additive Manufacturing of Magnetic Components for Power Electronics
   Integration},
Booktitle = {2016 INTERNATIONAL CONFERENCE ON ELECTRONICS PACKAGING (ICEP)},
Year = {2016},
Pages = {368-371},
Note = {International Conference on Electronics Packaging (ICEP), Sapporo,
   JAPAN, APR 20-22, 2016},
Organization = {Japan Inst Elect Packaging; IEEE Components Packaging \& Mfg Technol
   Soc, Japan Chapter; Int MIcroelectron \& Packaging Soc; ASE Grp; NAMICS;
