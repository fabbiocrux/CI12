Type = {Article},
Language = {English},
Affiliation = {Bartolo, P (Corresponding Author), Univ Manchester, Sch Engn, Dept Mech Aerosp \& Civil Engn, Manchester M13 9PL, England.
   Mirihanage, W (Corresponding Author), Univ Manchester, Sch Nat Sci, Dept Mat, Manchester M13 9PL, England.
