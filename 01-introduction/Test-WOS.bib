@article{ WOS:000705612900001,
Author = {Russell, Timothy and Jack, David A.},
Title = {Strength prediction in single beads of large area additive manufactured
   short-fiber polymers},
Journal = {POLYMER COMPOSITES},
Year = {2021},
Volume = {42},
Number = {12},
Pages = {6534-6550},
Month = {DEC},
Abstract = {The use of fiber reinforcement in large area additive manufactured
   components is of industrial interest due to the ability to enhance the
   structural properties of final processed parts. This work presents a
   methodology to predict the tensile, compressive, and flexural yield
   strength of single beads of a large-scale, 3D printed, short-fiber
   reinforced material. The methodology is built on the strength theory of
   Van Hattum and Bernardo, which combines the Tsai-Wu failure criteria
   with Advani and Tucker's orientation averaging technique, allowing fiber
   orientation flow model results to serve as direct input into strength
   predictions. Visual advantages of the methodology such as the ability to
   plot spatially varying strength constants and failure plots are
   demonstrated. In addition, an analysis of the methodology's sensitivity
   to various parameters is conducted.},
