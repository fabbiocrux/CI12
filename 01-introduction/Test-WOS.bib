@article{ WOS:000367046100004,
Author = {Xu, Xin and Meteyer, Simon and Perry, Nicolas and Zhao, Yaoyao Fiona},
Title = {Energy consumption model of Binder-jetting additive manufacturing
   processes},
Journal = {INTERNATIONAL JOURNAL OF PRODUCTION RESEARCH},
Year = {2015},
Volume = {53},
Number = {23},
Pages = {7005-7015},
Month = {DEC 2},
Abstract = {Considering the potential for new product design possibilities and the
   reduction of environmental impacts, Additive Manufacturing (AM)
   processes are considered to possess significant advantages for
   automotive, aerospace and medical equipment industries. One of the
   commercial AM techniques is Binder-jetting (BJ). This technique can be
   used to process a variety of materials including stainless steel,
   ceramic, polymer and glass. However, there is very limited research
   about this AM technology on energy consumption aspect. This paper
   presents a method to build an energy consumption model for printing
   stage of BJ process. Mathematical analyses are performed to find out the
   correlation between the energy consumption and geometry of the
   manufactured part. Based on the analyses, total energy consumption is
   calculated as a function of part geometry and printing parameters.
   Finally, test printing is performed to check the accuracy of the model.
   This process model provides a tool to optimise part geometry design with
   respect to energy consumption.},
