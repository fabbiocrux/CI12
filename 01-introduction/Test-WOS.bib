@article{ WOS:000545654900030,
Author = {Regalla, Srinivasa Prakash and Karwa, Sagar S. and Rajesh, Sreeram and
   Shyam, P. V. and Shrivastava, Prakash N.},
Title = {Strength and fracture behaviour of polymer matrix composite layered
   structures made by additive manufacturing},
Journal = {MATERIALS TODAY-PROCEEDINGS},
Year = {2020},
Volume = {28},
Number = {2},
Pages = {1030-1038},
Note = {2nd International Conference on Recent Advances in Materials and
   Manufacturing Technologies (IMMT), Dubai, U ARAB EMIRATES, NOV 20-22,
   2019},
Abstract = {In this paper, studies on the tensile strength, fracture initiation and
   propagation behaviour of composite material layered shell structures
   manufactured by extrusion based additive manufacturing (AM) is reported.
   To understand the basic mechanical behaviour, simple tension tests have
   been conducted on ASTM-D638 specimens of carbon fiber and natural fiber
   reinforced polymer matrix composites made by AM in different modes as
   well as directly in filament form using a filament extruder. The tensile
   strengths reveal that the internal defects unique to additive
   manufacturing, such as lack of fusion (LOF) sites and voids, play
