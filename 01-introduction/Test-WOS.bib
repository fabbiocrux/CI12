Title = {In-Situ Thermal Imaging for Single Layer Build Time Alteration in
   Large-Scale Polymer Additive Manufacturing},
Booktitle = {47TH SME NORTH AMERICAN MANUFACTURING RESEARCH CONFERENCE (NAMRC 47)},
Series = {Procedia Manufacturing},
