@article{ WOS:000805234700011,
Author = {Pandey, Rohit and Bux, Sohail and Shrivastava, Ashish and Choubey,
   Abhishek and Singh, Swapnil},
Title = {A process optimization of additive layer manufacturing processes for the
   production of polymer composite-based components},
