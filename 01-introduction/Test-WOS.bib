Title = {Additive Manufacturing of Photopolymers using the Texas Instruments DLP
   Lightcrafter},
Booktitle = {EMERGING DIGITAL MICROMIRROR DEVICE BASED SYSTEMS AND APPLICATIONS V},
Series = {Proceedings of SPIE},
